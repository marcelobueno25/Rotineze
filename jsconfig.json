{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "@components/*": ["src/components/*"],
        "@screens/*": ["src/screens/*"],
        "@assets/*": ["src/assets/*"],
        "@routes/*": ["src/routes/*"],
        "@redux/*": ["src/redux/*"],
        "@services/*": ["src/services/*"],
        "@theme/*": ["src/theme/*"],
        "@utils/*": ["src/utils/*"],
        "@constant/*": ["src/constant/*"],
      }
    }
  }